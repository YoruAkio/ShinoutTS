{
    "compilerOptions": {
        "target": "ESNext",
        "module": "commonjs",
        "baseUrl": ".",
        "outDir": "dist",
        "strict": true,
        "skipLibCheck": true,
        "paths": {
            "@root/*": ["./*"],
            "@commands/*": ["./Commands/*"],
            "@events/*": ["./Events/*"],
            "@handlers/*": ["./Structure/*"],
            "@structures/*": ["./Structures/*"],
            "@utils/*": ["./Utils/*"]
        }
    }
}
